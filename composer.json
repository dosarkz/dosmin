{
  "name": "dosarkz/laravel-admin",
  "description": "Laravel framework admin panel",
  "authors": [
    {
      "name": "Yerzhan Ashenov",
      "email": "ashenov.e@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "laravelcollective/html": "5.*"
  },
  "autoload": {
    "classmap": [
      "src/Facade",
      "src/Config",
      "src/Providers",
      "src/Commands"
    ],
    "psr-4": {
      "Dosarkz\\LaravelAdmin\\": "src/"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Dosarkz\\LaravelAdmin\\Providers\\AdminServiceProvider"
        ],
        "aliases": {
          "Admin": "Dosarkz\\LaravelAdmin\\Facade\\AdminFacade"
        }
      }
    }
  }
}